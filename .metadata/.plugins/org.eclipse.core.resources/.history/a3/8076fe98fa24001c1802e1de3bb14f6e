package es.cfc;

import es.cfc.models.Producto;

public class AppMain {

	public static void main(String[] args) {
		
		Producto producto1 = new Producto(0, "Ratón", 33.3, "Trust", "Ratón inalámbrico");
		Producto producto2 = new Producto(1, "Ratón", 33.3, "Trust", "Ratón inalámbrico");
		
		producto1.comprobar();
		producto2.comprobar();
	}

}
